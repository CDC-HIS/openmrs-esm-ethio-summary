{
  "$schema": "https://json.openmrs.org/routes.schema.json",
  "backendDependencies": {
    "fhir2": ">=1.2",
    "webservices.rest": "^2.24.0"
  },
  "extensions": [
    {
      "name": "Red box",
      "component": "redBox",
      "slot": "Boxes"
    },
    {
      "name": "Blue box",
      "component": "blueBox",
      "slot": "Boxes"
    },
        {
      "name": "Brand box",
